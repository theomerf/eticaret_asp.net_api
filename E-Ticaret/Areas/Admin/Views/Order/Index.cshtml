@model IEnumerable<OrderDto>

<div class="admin-header mt-4">
	<h1><i class="fa-solid fa-gears"></i> Sipariş Yönetimi</h1>
</div>

@if(Model?.Count() > 0)
{
	@foreach (var order in Model)
	{
		<div class="my-3">
			<partial name="_OrderDetails" model="@order" />
		</div>
	}
}
else
{
	<div class="alert alert-info my-5"> 
		<p class="lead">There is no order yet.</p>
	</div>
}
